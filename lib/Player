import 'package:flutter/material.dart';
// is this necessary^^^^^^ ? ^

// Player class for logic testing

class Player {
  int playerID;
  int targetID;
  String realName;
  String username;
  bool isEliminated;
  bool isTargeted;

  Player(pID,tID,rn,un,isE,isT)
  {
    this.playerID = pID;
    this.targetID = tID;
    this.realname = rn;
    this.username = un;
    this.isEliminated = isE;
    this.isTargeted = isT;
  }

}